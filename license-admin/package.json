{
  "name": "license-admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ro": "ng serve --configuration=ro",
    "start:prod": "ng serve --configuration=production",
    "start:prod:ro": "ng serve --configuration=production-ro",
    "build:prod": "ng build --configuration=production",
    "build:prod:ro": "ng build --configuration=production-ro",
    "test": "ng test --reporters=progress,junit --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar": "node analyse.js",
    "docker": "docker build .",
    "extract": "ng xi18n --output-path=locale",
    "extract:copy": "ng xi18n --output-path=locale && ts-node src/scripts/copyAndAdjustOriginalTranslationFile.ts",
    "translate:dummy": "ts-node src/scripts/copyAndAdjustOriginalTranslationFile.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/cdk": "7.1.1",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/material": "7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/router": "7.1.1",
    "core-js": "2.5.4",
    "hammerjs": "2.0.8",
    "rxjs": "6.3.3",
    "tslib": "1.9.0",
    "tslint-sonarts": "1.8.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.1",
